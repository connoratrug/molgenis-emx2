{
  "name": "catalogue",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@mswertz/emx2-styleguide": "*",
    "@vue/compat": "3.1.0",
    "core-js": "3.6.5",
    "graphql-request": "3.4.0",
    "vue": "3.1.0",
    "vue-router": "4.0.3",
    "vuex": "4.0.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "plugins": [
      "autofix"
    ],
    "rules": {
      "autofix/no-debugger": "error"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "devDependencies": {
    "webpack-graphql-loader": "1.0.2"
  }
}
